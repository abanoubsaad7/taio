          <label for="">product discreption</label>
          <br />
          <textarea name="proDescription" id=""cols="30"  rows="10" placeholder="product description"><%= objProduct.proDescription %></textarea>
          <br />
          <label for="">Product price</label>
          <br />
          <input name="proPrice" type="number" placeholder="product price" value="<%= objProduct.proPrice %>"/>
          <br />
          <label for="">product image</label>
          <input type="file"  name="proImg" id="files" class="btn lonbtn" />
          <br />
          <br />
          <input type="submit" value="update" class="btn btn-lg lonbtn" />
        </form></center>
      </div>
    </section>
    <section id="about">
      <center>
        <img src="/img/logo.png" alt="logo" class="logo" />
        <h2>اسرة مارجرجس اعدادي</h2>
        <h3>اسرة بتحبك ومستنياااااااااااااااااااك</h3>
      </center>
    </section>
  </body>
</html>
